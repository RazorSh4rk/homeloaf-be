  },
  "devDependencies": {
    "@smui/card": "^7.0.0-beta.8",
    "@smui/list": "^7.0.0-beta.8",
    "@sveltejs/vite-plugin-svelte": "^2.0.4",
    "smui-theme": "^7.0.0-beta.8",
    "svelte": "^3.58.0",
    "vite": "^4.3.9"
  },
  "dependencies": {
    "svelte-loading-spinners": "^0.3.4",
    "svelte-material-ui": "^7.0.0-beta.8",
    "sveltestrap": "^5.10.0"
  }
}
